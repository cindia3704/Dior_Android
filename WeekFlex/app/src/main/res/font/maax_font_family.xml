<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--regular-->
    <font
        android:font="@font/maax"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/maax"
        app:fontStyle="normal"
        app:fontWeight="500" />
    <!--bold-->
    <font
        android:font="@font/maaxbold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/maaxbold"
        app:fontStyle="normal"
        app:fontWeight="1000" />
    <!--medium-->
    <font
        android:font="@font/maaxmedium"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/maaxmedium"
        app:fontStyle="normal"
        app:fontWeight="1000" />

</font-family>